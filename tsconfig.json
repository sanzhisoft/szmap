{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es6",
      "dom"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictFunctionTypes": true,
    "baseUrl": ".",
    "noEmit": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "ignoreDeprecations": "5.0",
  },
}
